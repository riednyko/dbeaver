<plugin>

    <extension point="org.eclipse.ui.commands">
        <category id="org.jkiss.dbeaver.ext.db2" name="%category.db2.name" description="%category.db2.description"/>
        <command id="org.jkiss.dbeaver.ext.db2.table.reorg" name="%command.org.jkiss.dbeaver.ext.db2.table.reorg.name" description="%command.org.jkiss.dbeaver.ext.db2.table.reorg.name" categoryId="org.jkiss.dbeaver.ext.db2"/>
        <command id="org.jkiss.dbeaver.ext.db2.table.reorgix" name="%command.org.jkiss.dbeaver.ext.db2.table.reorgix.name" description="%command.org.jkiss.dbeaver.ext.db2.table.reorgix.name" categoryId="org.jkiss.dbeaver.ext.db2"/>
        <command id="org.jkiss.dbeaver.ext.db2.table.runstats" name="%command.org.jkiss.dbeaver.ext.db2.table.runstats.name" description="%command.org.jkiss.dbeaver.ext.db2.table.runstats.name" categoryId="org.jkiss.dbeaver.ext.db2"/>
    </extension>

    <extension point="org.eclipse.core.runtime.adapters">
        <factory adaptableType="java.lang.Object" class="org.jkiss.dbeaver.ext.db2.actions.DB2ObjectAdapter">
            <adapter type="org.jkiss.dbeaver.ext.db2.model.DB2Table"/>
            <adapter type="org.jkiss.dbeaver.ext.db2.model.DB2View"/>
        </factory>
    </extension>

    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="org.jkiss.dbeaver.db2.tableSelected">
            <with variable="selection">
                <count value="+"/>
                <iterate operator="and">
                    <adapt type="org.jkiss.dbeaver.ext.db2.model.DB2Table"/>
                </iterate>
            </with>
        </definition>
    </extension>

    <extension point="org.eclipse.ui.handlers">
       <handler commandId="org.jkiss.dbeaver.ext.db2.table.reorg" class="org.jkiss.dbeaver.ext.db2.actions.DB2TableToolsHandler">
            <enabledWhen><reference definitionId="org.jkiss.dbeaver.db2.tableSelected"/></enabledWhen>
       </handler>
       <handler commandId="org.jkiss.dbeaver.ext.db2.table.reorgix" class="org.jkiss.dbeaver.ext.db2.actions.DB2TableToolsHandler">
           <enabledWhen><reference definitionId="org.jkiss.dbeaver.db2.tableSelected"/></enabledWhen>
       </handler>
       <handler commandId="org.jkiss.dbeaver.ext.db2.table.runstats" class="org.jkiss.dbeaver.ext.db2.actions.DB2TableToolsHandler">
           <enabledWhen><reference definitionId="org.jkiss.dbeaver.db2.tableSelected"/></enabledWhen>
        </handler>
    </extension>

    <extension point="org.eclipse.ui.menus">
       <menuContribution allPopups="false" locationURI="popup:org.eclipse.ui.popup.any?after=tools">
          <menu label="%menu.tables.manage" icon="icons/tools.png">
             <command commandId="org.jkiss.dbeaver.ext.db2.table.runstats">
                <visibleWhen checkEnabled="true"/>
             </command>
             <command commandId="org.jkiss.dbeaver.ext.db2.table.reorg">
                <visibleWhen checkEnabled="true"/>
             </command>
             <command commandId="org.jkiss.dbeaver.ext.db2.table.reorgix">
                <visibleWhen checkEnabled="true"/>
             </command>
          </menu>
       </menuContribution>
    </extension>

    <extension
        point="org.jkiss.dbeaver.dataSourceProvider">
        <datasource
              class="org.jkiss.dbeaver.ext.db2.DB2DataSourceProvider"
              description="DB2 connector"
              icon="icons/db2_icon.png"
              id="db2"
              label="DB2">

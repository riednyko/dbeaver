<templates>

<template name="new" description="%Templates.new" id="org.eclipse.jdt.ui.templates.new" context="sql" enabled="true" autoinsert="false">${type} ${name} = new ${type}(${arguments});</template>

<template name="toarray" description="%Templates.toarray" id="org.eclipse.jdt.ui.templates.toarray" context="sql" enabled="true" autoinsert="false">(${type:elemType(collection)}[]) ${collection}.toArray(new ${type}[${collection}.size()])</template>

<template name="nls" description="%Templates.non-nls" id="org.eclipse.jdt.ui.templates.non-nls" context="sql" enabled="true" autoinsert="false">//$$NON-NLS-${N}$$</template>

<template name="runnable" description="%Templates.runnable" id="org.eclipse.jdt.ui.templates.runnable" context="sql" enabled="true" autoinsert="false">new Runnable() {
	public void run() {
		${line_selection}
	}
}</template>

</templates>

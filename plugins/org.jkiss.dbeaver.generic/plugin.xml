                    webURL="http://www.ingres.com/products/ingres-database/"
                    description="Ingres Database">
                    <file type="jar" path="drivers/ingres/iijdbc.jar" url="*"/>
                    <file type="license" path="drivers/ingres/LICENSE.txt" url="*"/>
                </driver>
                <driver
                    id="linter"

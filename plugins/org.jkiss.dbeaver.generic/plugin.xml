                    description="Intersystems Cache">
                    <file type="jar" path="drivers/cache/CacheDB.jar" url="*"/>
                    <file type="license" path="drivers/cache/LICENSE.txt" url="*"/>
                </driver>
                <driver
                    id="ingres"
                    label="Ingres"
                    class="com.ingres.jdbc.IngresDriver"
                    sampleURL="jdbc:ingres://{host}:{port}/{database}"

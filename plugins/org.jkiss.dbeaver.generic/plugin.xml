                <driver
                    id="cassandra_jkiss"
                    label="Cassandra"
                    class="org.jkiss.jdbc.cassandra.CassandraDriver"
                    sampleURL="jdbc:jkiss:cassandra://{host}[:{port}]/{database}"
                    defaultPort="9160"
                    webURL="http://cassandra.jkiss.org/"
                    description="Driver for Apache Cassandra">
                    <property name="structResultSet" value="true"/>
                    <file type="license" path="drivers/cassandra/license.txt" url="*"/>
                    <file type="jar" path="drivers/cassandra/cassandra-jkiss-jdbc-1.2.0.jar" url="*"/>
                    <file type="jar" path="drivers/cassandra/apache-cassandra-thrift-1.1.6.jar" url="*"/>
                    <file type="jar" path="drivers/cassandra/libthrift-0.7.0.jar" url="*"/>
                    <file type="jar" path="drivers/cassandra/slf4j-api-1.6.1.jar" url="*"/>
                </driver>
                <driver
                    id="csvjdbc"
                    label="CSV/DBF"
                    category="Flat files (CSV)"
                    class="org.relique.jdbc.csv.CsvDriver"
                    sampleURL="jdbc:relique:csv:{folder}"
                    webURL="http://csvjdbc.sourceforge.net/"
                    description="CsvJdbc driver for CSV and DBF files">
                    <parameter name="supports-select-count" value="false"/>
                    <property name="separator" value=","/>
                    <property name="quotechar" value="&quot;"/>
                    <property name="fileExtension" value="csv"/>
                    <property name="suppressHeaders" value="false"/>
                    <file type="license" path="drivers/csvjdbc/LICENSE.txt" url="*"/>
                    <file type="jar" path="drivers/csvjdbc/csvjdbc-1.0.12.jar" url="*"/>
                </driver>
                <driver
                    id="msaccess_ucanaccess"
                    label="UCanAccess"
                    category="MS Access"
                    class="net.ucanaccess.jdbc.UcanaccessDriver"
                    sampleURL="jdbc:ucanaccess://{file}"
                    webURL="http://ucanaccess.sourceforge.net/site.html"
                    description="Microsoft Access driver">
                    <file type="license" path="drivers/ucanaccess/LICENSE.txt" url="*"/>
                    <file type="jar" path="drivers/ucanaccess/ucanaccess-1.0.1.jar" url="*"/>
                    <file type="jar" path="drivers/ucanaccess/commons-lang-2.4.jar" url="*"/>
                    <file type="jar" path="drivers/ucanaccess/hsqldb.jar" url="*"/>
                    <file type="jar" path="drivers/ucanaccess/jackcess-1.2.1.jar" url="*"/>
                </driver>
            </drivers>

